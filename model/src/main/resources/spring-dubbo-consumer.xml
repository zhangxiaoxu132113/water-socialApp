<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="mw-es-customer" owner="mwes" organization="mwes"/>

    <!-- 测试 -->
    <dubbo:registry protocol="zookeeper" address="127.0.0.1:2181" check="false"/>

    <dubbo:reference id="esArticleService" timeout="60000" url="dubbo://127.0.0.1:20880" interface="com.water.es.api.Service.IArticleService" protocol="dubbo"/>
</beans>